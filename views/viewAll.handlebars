<div class="row"></div>
<div class="container">
    <div class="row center-align">
        <a href="#!" id = "cymbal-menu-On-view">
            <div class="col s12 m3">
                <div class="card-panel hoverable menuOnView">
                    <i class="fas fa-search-plus"> Cymbals</i>
                </div>
            </div>
        </a>
        <a href="#!" id = "shure-menu-On-view">
            <div class="col s12 m3">
                <div class="card-panel hoverable menuOnView">
                    <i class="fas fa-search-plus"> Shure Parts</i>
                </div>
            </div>
        </a>
    </div>
</div>
<div class="container" id="searchWindowOnView"></div>
<div class="container">
    <table class="highlight centered">
        <thead style="font-weight: bold;">
            <tr>
                <th>Package Code</th>
                <th>Category01</th>
                <th>Category02</th>
                <th>Size</th>
                <th>Description</th>
                <th>Code</th>
                <th>QTY</th>
                <th>eBay Price</th>
                <th>Website Price</th>
            </tr>
        </thead>

        <tbody>
            {{#each allItems}}
            <tr>
                <td><button class="cymbal-package-btn" data-id="{{this.group_id}}">{{this.group_code}}</button></td>
                <td>{{this.category01}}</td>
                <td>{{this.category02}}</td>
                <td>{{this.size}}</td>
                <td>{{this.description}}</td>
                <td>{{this.code}}</td>
                <td>{{this.qty}}</td>
                <td>$ {{this.ebay_price}}</td>
                <td>$ {{this.website_price}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <div id="displayPackwithCymbals"></div>



</div>
<div class="row"></div>